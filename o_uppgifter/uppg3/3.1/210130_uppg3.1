With Ada.Text_IO, Ada.Integer_Text_IO, Ada.Float_Text_IO;
use Ada.Text_IO, Ada.Integer_Text_IO, Ada.Float_Text_IO;

Procedure uppgen is
       
   type Inner is 
     array (1..4) of String (1..7); 
   -----------------------------------------------------
   
   type Outer is 
     array (-6..1) of Inner;
   ----------------------------------------------------
   
   A : Outer;
   C : Character;

begin
   
   Put_Line ("Mata in data:");
   
   for X in Outer'Range loop
      for Y in Inner'Range loop
	 Get (A (X)(Y));
	 if Y < 4 then 
	    Get (C);
	 end if;
      end loop;
   end loop;
   
   
   Put_Line ("Följande data finns i fältet:");
   
   for C in Outer'Range loop
      for K in Inner'Range loop
	 Put (A (C)(K));
	 Put (' ');
      end loop;
      New_Line;
   end loop;
   
end uppgen;
